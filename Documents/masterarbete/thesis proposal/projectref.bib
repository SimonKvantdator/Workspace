@article{KalMcLWil92a,
  author={Kalnins, E. G. and McLenaghan, R. G. and Williams, G. C.},
  title={Symmetry Operators for {Maxwell's} Equations on Curved Space-Time},
  volume={439},
  number={1905},
  pages={103-113},
  year={1992},
  doi={10.1098/rspa.1992.0136},
  abstract={We derive necessary and sufficient conditions that a second-order co-variant
    differential operator be a symmetry operator of Maxwell's equations in a general
    curved space-time background. It is found that such operators are naturally
    formulated in terms of conformal Killing vectors, tensors and spinors. Operators
    of this type play a role in the solution of Maxwell's equations via separation
    of variables in the Kerr background space-time.},
  URL={http://rspa.royalsocietypublishing.org/content/439/1905/103.abstract},
  journal={Proc. R. Soc. Lond. A}
}

