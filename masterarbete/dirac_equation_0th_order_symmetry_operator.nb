Cell[BoxData[
 RowBox[{
  RowBox[{"TexPrintLongLHSAligned", "[", 
   RowBox[{"exprs_List", ",", "alignstr_String"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"lhstex", "=", 
      RowBox[{"TexPrint", "@", 
       RowBox[{"exprs", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], "}"}], ",", 
    RowBox[{"StringReplace", "[", 
     RowBox[{
      RowBox[{"TexPrintAlignedEquations", "[", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\"\<eXpR\>\"", "==", 
           RowBox[{"exprs", "[", 
            RowBox[{"[", "2", "]"}], "]"}]}], "}"}], ",", 
         RowBox[{"Rest", "@", 
          RowBox[{"Rest", "@", "exprs"}]}]}], "]"}], "]"}], ",", 
      RowBox[{"\"\<eXpR\>\"", "\[Rule]", 
       RowBox[{"StringJoin", "[", 
        RowBox[{
         RowBox[{"StringInsert", "[", 
          RowBox[{"lhstex", ",", "\"\<&\>\"", ",", 
           RowBox[{
            RowBox[{"Last", "@", 
             RowBox[{"First", "@", 
              RowBox[{"StringPosition", "[", 
               RowBox[{"lhstex", ",", "alignstr", ",", "1"}], "]"}]}]}], "+", 
            "1"}]}], "]"}], ",", "\"\<\\\\nonumber\\\\\\\\\\n\>\""}], 
        "]"}]}]}], "]"}]}], "]"}]}]], "Input",
